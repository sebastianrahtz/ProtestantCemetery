         <div type="inscription" xml:lang="fr" decls="#f_T #c_2 #m_I #t_">
            <ab>COMPTE ALBERT DE KEYSERLING</ab>
            <ab>SEIGNER DE SUTTEN ET KAHNHOF EN COURLANDE</ab>
            <ab>NÉ A KABILLEN LE 14 FÉVRIER 1843</ab>
            <ab>MORT A ROME LE 11 DECEMBRE 1907</ab>
            <ab/>
            <ab rend="Alignl">"VOICI,MES SOUFFRANCES MEMES SONT DEVENUES</ab>
            <ab rend="Alignl">MON SALUT,TU AS PRIS PLAISIR A RETIRER</ab>
            <ab rend="Alignl">MON ÂME DE LA FOSSE DU NÉANT, CAR TU</ab>
            <ab rend="Alignl">AS JETÉ DERRIÉRE TOI TOUS MES PÉCHÉS."</ab>
            <ab rend="Alignr">ESAI XXXVIII.17.</ab>
         </div>
         <div type="inscription" xml:lang="it" decls="#f_S #c_2 #m_I #t_S">
            <ab>BARONESSA</ab>
            <ab>EMMA CATERINA DE HAHN</ab>
