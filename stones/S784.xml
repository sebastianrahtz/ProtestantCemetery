         <div type="inscription" xml:lang="en" decls="#f_S #c_1 #m_IF #t_P">
            <ab>SACRED</ab>
            <ab>TO THE MEMORY OF</ab>
            <ab>JAMES SHEPHERD</ab>
            <ab>DIED IN ROME</ab>
            <ab>THE 7TH OF MAY 1869</ab>
            <ab>AGED 66 YEARS</ab>
            <ab>BLESSED ARE THE MERCIFUL</ab>
            <ab>FOR THEY SHALL OBTAIN MERCY</ab>
            <ab rend="Alignr">MATTHEW 5 VERSE 7</ab>
         </div>
         <div type="inscription" xml:lang="en" decls="#f_N #c_1 #m_IF #t_S">
            <ab>SACRED TO THE MEMORY OF</ab>
            <ab>FRANCES ELIZABETH</ab>
            <ab>WIDOW OF JAMES SHEPHERD</ab>
            <ab>WHO</ab>
            <ab>(HAVING IN 1882 INTERMARRIED WITH)</ab>
            <ab>(COUNT CANSACCHI WHO DIED IN 1890)</ab>
            <ab>DEPARTED THIS LIFE AT ROME</ab>
            <ab>THE 15TH OF JULY 1891</ab>
            <ab>AGED 80 YEARS</ab>
         </div>
