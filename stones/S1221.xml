   <text>
      <body>
         <div type="inscription" xml:lang="de" decls="#f_N #c_3 #m_IF #t_P">
            <ab>
               <hi rend="gothic">Ioh. Iak. Ernst Corrodi,</hi>
            </ab>
            <ab>
               <hi rend="gothic">Geb. in Rom den 9. Nov. 1851</hi>
            </ab>
            <ab>
               <hi rend="gothic">Gest. in Sorrento den 27. Juni 1852</hi>
            </ab>
            <ab>
               <hi rend="gothic">Begr. in Scafati bei Neapel.</hi>
            </ab>
         </div>
         <div type="inscription" xml:lang="de" decls="#f_W #c_3 #m_IF #t_P">
            <ab>
               <hi rend="gothic">El. Katharina Corrodi,</hi>
            </ab>
            <ab>
               <hi rend="gothic">Geb. den 10. April 1853,</hi>
            </ab>
            <ab>
               <hi rend="gothic">Gest. den 12. April 1853.</hi>
            </ab>
         </div>
         <div type="inscription" xml:lang="de" decls="#f_E #c_3 #m_IF #t_P">
            <ab>
               <hi rend="gothic">Kath. Em. Elisa Corrodi</hi>
            </ab>
            <ab>
               <hi rend="gothic">Geb. den 10. April 1853.</hi>
            </ab>
            <ab>
               <hi rend="gothic">Gest. den 2. Sept. 1856</hi>
            </ab>
         </div>
         <div type="inscription" xml:lang="de" decls="#f_S #c_3 #m_IF #t_PC">
            <ab>
               <hi rend="gothic">Und wenn [emol] der Sunntigstagt,</hi>
            </ab>
            <ab>
               <hi rend="gothic">Und d'Engel singt's Morgelied,</hi>
            </ab>
            <ab>
               <hi rend="gothic">Si stohn mir mit inander uf,</hi>
            </ab>
            <ab>
               <hi rend="gothic">Erquickt und gsund!</hi>
            </ab>
            <ab/>
            <ab>
               <hi rend="gothic">Zum andenken</hi>
            </ab>
            <ab>
               <hi rend="gothic">Dreier ihr ihrurrn Geschwister</hi>
            </ab>
            <ab>
               <hi rend="gothic">von ihren Brudern</hi>
            </ab>
         </div>
      </body>
